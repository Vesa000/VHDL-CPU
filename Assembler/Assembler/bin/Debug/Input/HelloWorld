
HELLOWORLD:
	LDV	72	1		// H
	BRA	PRINT
	LDV	69	1		// E
	BRA	PRINT
	LDV	76	1		// L
	BRA	PRINT
	LDV	76	1		// L
	BRA	PRINT
	LDV	79	1		// O
	BRA	PRINT
	LDV	32	1		// SPACE
	BRA	PRINT
	LDV	87	1		// W
	BRA	PRINT
	LDV	79	1		// O
	BRA	PRINT
	LDV	82	1		// R
	BRA	PRINT
	LDV	76	1		// L
	BRA	PRINT
	LDV	68	1		// D
	BRA	PRINT
	LDV	10	1		// \n
	BRA	PRINT
	LDV	13	1		// \r
	BRA	PRINT

	JMP	HELLOWORLD


PRINT:
	LDR	65537	2		// Uart status register
	LDV	1	3		// Uart Ready
	LDV	3	4		// Uart Ready and Uart Available

	CMP	2	3
EQ	JMP	PRINTEND
	CMP	2	4

EQ	JMP	PRINTEND
	
	JMP	PRINT


PRINTEND:
	
	STR	1	65536		// Write to uart
	RET